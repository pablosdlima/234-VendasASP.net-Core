@model IEnumerable<IGrouping<Departamento, VendasRecord>>
@{
    ViewData["Title"] = "Buscar pro Grupo";
}

<h2>@ViewData["Title"]</h2>
<nav class="navbar navbar-inverse">
    <div class="container-fluid">
        <form class="navbar-form navbar-left" role="search">
            <div class="form-group">
                <div class="form-group">
                    <label for="minData">Data Minima</label>
                    <input type="date" class="form-control" name="minData" value=@ViewData["minDate"] />
                </div>
                <div class="form-group">
                    <label for="maxData">Max Date</label>
                    <input type="date" class="form-control" name="maxData" value=@ViewData["maxDate"] />
                </div>
            </div>
            <button type="submit" class="btn btn-success">Buscar</button>
        </form>
    </div>
</nav>
<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">Total</h3>
    </div>
    <div class="panel-body">
    </div>
</div>
